<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="co.edu.uptc.viewController.MainController">

    <top>
        <ToolBar>
            <Button text="Cargar XML" onAction="#onLoadGraph"/>
            <Button text="Mostrar Nodos" onAction="#onShowNodes"/>
        </ToolBar>
    </top>

    <center>
        <TextArea fx:id="outputArea" editable="false" wrapText="true"/>
    </center>

</BorderPane>
